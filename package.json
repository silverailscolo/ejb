  "bugs": {
    "url": "https://github.com/silverailscolo/ejb/issues"
  },
  "homepage": "https://silverailscolo.github.io/ejb",
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^1.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.7.1",
    "lightgallery": "^2.7.2"
  }
}
